{
  "name": "@firebase-ui/core",
  "version": "0.0.1",
  "description": "Core authentication service for Firebase UI",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:local": "pnpm run build && pnpm pack",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "firebase",
    "auth",
    "authentication",
    "firebaseui"
  ],
  "author": "TODO",
  "license": "MIT",
  "peerDependencies": {
    "firebase": "^11"
  },
  "dependencies": {
    "nanostores": "^0.11.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  }
}
