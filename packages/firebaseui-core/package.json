{
  "name": "firebaseui-core",
  "version": "0.1.0",
  "description": "Core authentication service for Firebase UI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist"
  },
  "keywords": [
    "firebase",
    "auth",
    "authentication",
    "firebaseui"
  ],
  "author": "Invertase",
  "license": "MIT",
  "peerDependencies": {
    "firebase": "^10.0.0"
  },
  "dependencies": {
    "nanostores": "^0.9.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "tsup": "^8.0.1",
    "prettier": "^3.1.1",
    "@types/node": "^20.10.5",
    "rimraf": "^6.0.1"
  }
}
