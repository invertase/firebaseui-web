{
  "name": "@firebase-ui/styles",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": [
    "dist.css",
    "src"
  ],
  "scripts": {
    "postinstall": "pnpm run build",
    "build": "npx -y @tailwindcss/cli -i ./src.css -o ./dist.css --minify",
    "build:local": "pnpm run build && pnpm pack",
    "release": "pnpm pack --pack-destination ../../releases/"
  },
  "devDependencies": {
    "tailwindcss": "^4.0.0"
  }
}
